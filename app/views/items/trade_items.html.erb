<h1>自分の出品物</h1>
<% if @items.blank? %>
  <p>出品物がありません</p>
  <%=link_to("出品する","/items/new")%>
<% else %>
  <% @items.each do |item| %>
    <%= link_to(item.title,"/items/#{HASHIDS.encode(item.id)}") %>
    <%= link_to("この商品と物々交換する", "/items/#{HASHIDS.encode(@item_requested.id)}/#{HASHIDS.encode(item.id)}/trade", data: { turbo_method: "post" }) %>
    <br>
  <% end %>

<% end %>